---
import Box from "../box";
import type { HTMLTag, ComponentProps } from "astro/types";
interface Props extends ComponentProps<typeof Box> {}
const { as: As = "p", ...props } = Astro.props;
const properties = { as: As, ...props };
---

<Box class:list={["card-text", Astro.props.class]} {...properties}>
  <slot />
</Box>
