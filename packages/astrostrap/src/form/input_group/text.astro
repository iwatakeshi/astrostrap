---
import Box from "../../box";
import type { ComponentProps } from "astro/types";
interface Props extends ComponentProps<typeof Box> {
}
const {
    as: As = "span",
    class: klass,
    "class:list": klassList,
    ...props
} = Astro.props as Props;
---

<As class:list={["input-group-text", klass, klassList]} {...props}><slot /></As>